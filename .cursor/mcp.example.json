{
    "mcpServers": {
        "crew-name-stdio": {
            "type": "stdio",
            "command": "/path/to/your/.venv/bin/python",
            "args": [
                "/path/to/your/mcp_server.py"
            ],
            "env": {
                "OPENAI_API_KEY": "sk-",
                "SERPER_API_KEY": ""
            }
        },
        "crew-name-sse": {
            "type": "sse",
            "url": "http://localhost:8000/sse"
        }
    }
}